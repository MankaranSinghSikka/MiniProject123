@model List< SharingEconomyPlatform.Models.Category>
@{
    ViewBag.Title = "Cetegory";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Category</h2>
<button class="btn btn-primary" onclick="location.href='AddCategory'">Add Product</button> 


<table class="table">
    <tr>
        <th>Name</th>
        <th>Description</th>
    </tr>
    @foreach(var item in Model)
            {
                <tr>
                    <td>@item.Name</td>
                    <td>@item.Description</td>
                </tr>
            }
</table>